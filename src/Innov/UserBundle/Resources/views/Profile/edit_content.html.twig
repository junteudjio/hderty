

<div class="col-lg-6">
                      <section class="panel">
                          <header class="panel-heading">
                              Modifier Profile
                          </header>
                          <div class="panel-body">
                              <form role="form" action="{{ path('fos_user_profile_edit') }}"  {{ form_enctype(form) }} method="POST">
                                  <div class="form-group">
                                      <label for="exampleInputEmail1">prenom </label>
                                      {{ form_widget(form.prenom , {'attr':{'class':'form-control' }}) }}
                                  </div>
                                 <div class="form-group">
                                      <label for="exampleInputEmail1">nom </label>
                                      {{ form_widget(form.nom , {'attr':{'class':'form-control'}}) }}
                                  </div>
                                  <div class="form-group">
                                      <label for="exampleInputEmail1">Email </label>
                                      {{ form_widget(form.email , {'attr':{'class':'form-control'}}) }}
                                  </div>
                                  
                                  <div class="form-group">
                                      <label for="exampleInputEmail1">votre ecole </label>
                                      {{ form_widget(form.ecole , {'attr':{'class':'form-control'}}) }}
                                  </div>
                                 
                                  <div class="form-group">
                                      <label for="exampleInputFile">Photo</label>
                                      {{ form_widget(form.photo ) }}
                                      <p class="help-block"> pour que vos camarades puissent vous reconna√Ætre aisement</p>
                                  </div>
                                  
                                   <div class="form-group">
                                      <label for="exampleInputEmail1"> veillez entrer votre mot de passe actuel </label>
                                      {{ form_widget(form.current_password , {'attr':{'class':'form-control'}}) }}
                                  </div>
                                      
                                  <button type="submit" class="btn btn-info">Submit</button>
                                    {{ form_rest(form) }}
                              </form>
                                  

                          </div>
                      </section>
                  </div>